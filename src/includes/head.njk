{%- set absolutePageUrl -%}{{ page.url | url | absoluteUrl(meta.url) }}{%- endset -%}
{% set reset %}{% include "reset.css" %}{% endset %}
{% set critical %}{% include "critical.css" %}{% endset %}
{% set fonts %}{% include "fonts.js" %}{% endset %}

<base href="{{ absolutePageUrl }}" />
<meta charset=utf-8>
<meta http-equiv=x-ua-compatible content="ie=edge">
<meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
<meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1,maximum-scale=5,viewport-fit=cover">
<title>{{ meta.title }}</title>
<link rel="canonical" href="{{ absolutePageUrl }}" />
<meta name="description" content="{{ meta.description }}">
<meta property="author" content="{{ meta.author }}">
<meta property="og:description" content="{{ meta.description }}">
<meta property="og:site_name" content="{{ meta.title }}">
<meta property="og:title" content="{{ meta.title }}">
<meta property="og:type" content="website">
<meta property="og:image" content="{{ meta.url }}/assets/images/main.jpeg">
<meta property="og:image:secure_url" content="{{ meta.url }}/assets/images/main.jpeg">
<meta property="og:image:width" content="1200">
<meta property="og:image:height" content="630">
<meta property="og:image:type" content="image/jpeg">
<meta property="og:url" content="{{ absolutePageUrl }}">
<meta property="twitter:site" content="{{ meta.social }}">
<meta name=twitter:image content="{{ meta.url }}/assets/images/main.jpeg">
<meta name=twitter:card content="summary_large_image">
<meta name=twitter:description content="{{ meta.description }}">
<meta name=twitter:title content="{{ meta.title }}">
<script type="application/ld+json">
  {
    "@context":"http://schema.org",
    "@type":"WebSite",
    "name":"{{ meta.title }}",
    "url":"{{ absolutePageUrl }}"
  }
</script>
<meta name="image" content="{{ meta.url }}/assets/images/about.png">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="black">
<meta name="apple-mobile-web-app-title" content="{{ meta.title }}">
<link rel="icon" type=image/png sizes="16x16" href="{{ meta.url }}/assets/icons/favicon-16x16.png">
<link rel="icon" type=image/png sizes="32x32" href="{{ meta.url }}/assets/icons/favicon-32x32.png">
<link rel="apple-touch-icon" sizes="120x120" href="{{ meta.url }}/assets/icons/icon-120x120.png">
<link rel="apple-touch-icon" sizes="152x152" href="{{ meta.url }}/assets/icons/icon-152x152.png">
<link rel="apple-touch-icon" sizes="167x167" href="{{ meta.url }}/assets/icons/icon-167x167.png">
<link rel="apple-touch-icon" sizes="180x180" href="{{ meta.url }}/assets/icons/icon-180x180.png">
<link rel="apple-touch-icon" sizes="256x256" href="{{ meta.url }}/assets/icons/icon-256x256.png">
<link rel="apple-touch-icon" sizes="512x512" href="{{ meta.url }}/assets/icons/icon-512x512.png">
<link rel="mask-icon" color=#c390f3 href="{{ meta.url }}/assets/icons/safari-pinned-tab.svg">
<link rel="manifest" href="{{ meta.url }}/manifest.json">
<meta name="theme-color" content="#9580ff">
<link rel="preload" href="{{ meta.url }}/assets/fonts/MD-Nichrome-Black-sub.woff2" as="font" type="font/woff2" crossorigin="anonymous">
<link rel="preload" href="{{ meta.url }}/assets/fonts/MD-Nichrome-Black-sub.woff" as="font" type="font/woff" crossorigin="anonymous">
<link rel="preload" href="{{ meta.url }}/assets/fonts/MD-Nichrome-Black-Oblique-sub.woff2" as="font" type="font/woff2" crossorigin="anonymous">
<link rel="preload" href="{{ meta.url }}/assets/fonts/MD-Nichrome-Black-Oblique-sub.woff" as="font" type="font/woff" crossorigin="anonymous">
<link rel="preload" href="{{ meta.url }}/assets/fonts/MD-Nichrome-Dark-sub.woff2" as="font" type="font/woff2" crossorigin="anonymous">
<link rel="preload" href="{{ meta.url }}/assets/fonts/MD-Nichrome-Dark-sub.woff" as="font" type="font/woff" crossorigin="anonymous">
<link rel="preload" href="{{ meta.url }}/assets/fonts/Nunito-Regular-sub.woff2" as="font" type="font/woff2" crossorigin="anonymous">
<link rel="preload" href="{{ meta.url }}/assets/fonts/Nunito-Regular-sub.woff" as="font" type="font/woff" crossorigin="anonymous">
<link rel="preload" href="{{ meta.url }}/assets/fonts/Nunito-Bold-sub.woff2" as="font" type="font/woff2" crossorigin="anonymous">
<link rel="preload" href="{{ meta.url }}/assets/fonts/Nunito-Bold-sub.woff" as="font" type="font/woff" crossorigin="anonymous">
<style>{{ reset | cssmin | safe }}{{ critical | cssmin | safe }}</style>
<script>{{ fonts | jsmin | safe }}</script>
<link rel="stylesheet" href="{{ meta.url }}/assets/styles/tw.css" />